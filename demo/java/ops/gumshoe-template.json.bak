{
  "template" : "gumshoe-*",
  "settings" : {
    "index" : {
      "refresh_interval" : "5s"
    }
  },
  "mappings": {
    "event": {
      "properties": {
        "$hostname": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$application": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$pid": {
          "type": "integer"
        },
        "$thread": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$stream_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$context": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$event_type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "$execution_time": {
          "type": "integer"
        },
        "$emitted_at": {
          "type": "date"
        }
      }
    }
  }
}

